  "metadata": {
    "version": "1.0",
    "description": "Configuration de mapping pour l'import/export F-Pack Matrix",
    "notes": [
      "Les colonnes avec target 'ignore' ne sont pas traitées lors de l'import",
      "Les colonnes avec target 'selection' créent des entrées dans FPM_projet_selection",
      "Les colonnes avec target 'sous_projet_fpack.*' remplissent directement la table FPM_sous_projet_fpack",
      "Le type 'fuzzy_match' utilise la similarité de chaîne pour trouver des correspondances",
      "Le type 'exact_match' cherche une correspondance exacte",
      "Le type 'direct' copie la valeur directement sans transformation"
    ]
  },
  "validation_rules": {
    "required_fields": [
      "FPack Number",
      "Robot location code"
    ],
    "optional_fields": [
      "Contractor",
      "Required Delivery time",
      "Delivery site"
    ],
    "fuzzy_match_threshold": 70,
    "exact_match_case_sensitive": false
  },
